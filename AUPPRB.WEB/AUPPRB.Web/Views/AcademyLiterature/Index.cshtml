@using AUPPRB.Models.ViewModels.jqGridModels
@using Trirand.Web.Mvc
@model AUPPRB.Models.ViewModels.jqGridModels.LiteratureMetaJqGridModel

@{
    ViewBag.Title = "Литература академии";
   
}
<h2>@ViewBag.Title</h2>


<div style="padding-top: 20px;" class="literatureGrid">
