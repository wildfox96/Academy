@{ Html.RenderPartial("_GridResources", new JqGridSettingsModel() { JqGridName = "LiteratureGrid", NeedEditButton = false,NeedDeleteButton = false,NeedViewButton = true, NeedViewWrappingDialog = true, ViewDialogTitle = "Основная информация о книге"}); }




